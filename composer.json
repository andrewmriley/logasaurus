{
  "name": "andrewmriley/logasaurus",
  "type": "library",
  "description": "Compiles changelog entries",
  "keywords": ["changelog"],
  "homepage": "https://yourcompany.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Riley",
      "email": "code@andrewmriley.dev",
      "homepage": "http://developerkarma.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "symfony/console": "^3",
    "symfony/yaml": "3.4.41",
    "symfony/filesystem": "^3",
    "symfony/finder": "^3"
  },
  "autoload": {
    "psr-4": {
      "andrewmriley\\logasaurus\\": "src"
    },
    "classmap": ["src"]
  },
  "bin": [
    "bin/logasaurus"
  ],
  "scripts": {
    "post-install-cmd": [
      "andrewmriley\\logasaurus\\Composer\\InstallBin::install"
    ],
    "post-update-cmd": [
      "andrewmriley\\logasaurus\\Composer\\InstallBin::install"
    ]
  },
  "require-dev": {
    "composer/composer": "^2.1"
  }
}
