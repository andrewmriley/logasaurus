#!/usr/bin/env php
<?php

$autoload = getcwd() . '/vendor/autoload.php';
if (is_file($autoload)) {
  require $autoload;
}

use Symfony\Component\Console\Application;
use andrewmriley\logasaurus\Commands\GenerateCommand;

$command = new GenerateCommand();

$app = new Application();
$app->add($command);
$app->run();
