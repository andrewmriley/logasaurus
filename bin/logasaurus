#!/usr/bin/env php
<?php
if (is_file(__DIR__.'/../autoload.php') === true) {
  include_once __DIR__.'/../autoload.php';
} else {
  include_once 'andrewmriley/logasaurus/autoload.php';
}

use Symfony\Component\Console\Application;
use andrewmriley\logasaurus\App\Commands\GenerateCommand;

$command = new GenerateCommand();

$app = new Application();
$app->add($command);
$app->run();
